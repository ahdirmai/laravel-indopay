{
  "name": "ahdirmai/laravel-indopay",
  "description": "Opinionated Laravel package for Indonesian payment gateways.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Author",
      "email": "author@example.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0",
    "moneyphp/money": "^4.0",
    "barryvdh/laravel-dompdf": "^2.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "IndoPay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IndoPay\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "IndoPay\\PaymentServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
